{
    "builds" : [
        {
            "src": "dcfence/wsgi.py",
            "use": "@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python 3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dcfence/wsgi.py"
        }
    ]
}